</div>


<h3>{{title}}<br>{{content}}</h3>

<form class="form new-project-form">
      <div class="form-group">
        <label for="review-rating">Rate the Joke:</label><div id="rateNum review-rating" class="rate rating"></div>
        <div class="form-group">
          <label for="review-content">What do you think?</label>
        <input class="form-input" type="text" id="review-content" name="review-content" />
        <div class="form-group">
        <button id="jokeReview" class="btn btn-primary" type="submit">Submit</button>
      </div>
</div>

 {{#each reviews as |review|}}
    <div class="row mb-2">
      <div class="col-md-8">
        <h3>here: {{review.content}}</h3>
        <p>score: {{review.rating}}/5 </p>
      </div>
    </div>
    {{/each}}

    <script src="/js/reviews.js"></script>